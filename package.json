{
  "name": "membership-app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build2": "tsc -b && vite build",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    "deploy:config:local": "npm run build && wrangler deploy --config wrangler.local.jsonc",
    "db:migrate:local": "wrangler d1 migrations apply membership-app-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply membership-app-db --remote",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "hono": "^4.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.12",
    "worker-mailer": "^1.1.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.2.3",
    "@cloudflare/workers-types": "^4.20250828.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.2",
    "tsc": "^2.0.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5",
    "vite-ssr-components": "^0.3.0",
    "wrangler": "^4.17.0"
  }
}
